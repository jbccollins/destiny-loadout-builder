		"react-redux": "8.0.4",
		"sharp": "^0.32.6",
		"uuid": "9.0.0"
	},
	"devDependencies": {
		"@babel/plugin-transform-typescript": "7.19.3",
		"@testing-library/dom": "8.19.0",
		"@testing-library/react": "13.4.0",
		"@testing-library/user-event": "14.4.3",
		"@types/autosuggest-highlight": "3.2.0",
		"@types/jest": "29.2.1",
